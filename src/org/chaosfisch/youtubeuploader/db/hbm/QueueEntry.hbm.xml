<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.chaosfisch.youtubeuploader.db.QueueEntry" table="QUEUE" schema="PUBLIC">
		<id name="identity" type="int" column="identity">
			<generator class="org.hibernate.id.IdentityGenerator"/>
		</id>
		<property name="archived" type="boolean" column="archived"/>
		<property name="category" type="java.lang.String" column="category"/>
		<property name="comment" type="short" column="comment"/>
		<property name="commentvote" type="boolean" column="commentvote"/>
		<property name="description" type="text" column="description"/>
		<property name="embed" type="boolean" column="embed"/>
		<property name="failed" type="boolean" column="failed"/>
		<property name="file" type="java.lang.String" column="file"/>
		<property name="keywords" type="text" column="keywords"/>
		<property name="mimetype" type="java.lang.String" column="mimetype"/>
		<property name="mobile" type="boolean" column="mobile"/>
		<property name="privatefile" type="boolean" column="privatefile"/>
		<property name="rate" type="boolean" column="rate"/>
		<property name="title" type="java.lang.String" column="title"/>
		<property name="unlisted" type="boolean" column="unlisted"/>
		<property name="uploadurl" type="java.lang.String" column="uploadurl"/>
		<property name="videoresponse" type="short" column="videoresponse"/>
		<property name="sequence" type="int" column="sequence"/>
		<property name="started" type="java.util.Date" column="started"/>
		<property name="inprogress" type="boolean" column="inprogress"/>
		<many-to-one name="account" column="account_id" cascade="all" not-null="false" lazy="false" class="org.chaosfisch.youtubeuploader.db.AccountEntry"/>
		<many-to-one name="playlist" lazy="false" class="org.chaosfisch.youtubeuploader.db.PlaylistEntry"/>
	</class>
</hibernate-mapping>