<?import de.chaosfisch.uploader.renderer.ImageButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>
<VBox id="VBox" fx:id="view" alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
      prefHeight="-1.0" prefWidth="-1.0" spacing="5.0" xmlns:fx="http://javafx.com/fxml/1"
      xmlns="http://javafx.com/javafx/2.2" fx:controller="de.chaosfisch.uploader.controller.AccountAddDialogController">
    <StackPane id="StackPane" styleClass="menu-bar">
        <Label fx:id="title" textFill="WHITE"/>
        <ImageButton onAction="#closeDialog" StackPane.alignment="CENTER_RIGHT">
            <image>
                <Image url="@../resources/images/close_control.png"/>
            </image>
        </ImageButton>
    </StackPane>
    <VBox>
        <WebView fx:id="webView"/>
    </VBox>
</VBox>
