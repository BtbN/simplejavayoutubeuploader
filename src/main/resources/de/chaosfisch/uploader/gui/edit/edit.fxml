		   xmlns="http://javafx.com/javafx/2.2">
	<items>
		<EditLeftView/>
		<GridPane hgap="5.0" vgap="5.0">
			<children>
				<EditRightView GridPane.columnSpan="3" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
				<Button mnemonicParsing="false" text="Ok, nÃ¤chster Upload!" GridPane.columnIndex="2"
