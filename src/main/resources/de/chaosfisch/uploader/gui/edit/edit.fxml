			</children>
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES"/>
				<ColumnConstraints hgrow="NEVER" maxWidth="-Infinity"/>
				<ColumnConstraints hgrow="NEVER" maxWidth="-Infinity"/>
			</columnConstraints>
			<rowConstraints>
				<RowConstraints/>
				<RowConstraints/>
			</rowConstraints>
		</GridPane>
	</items>
</SplitPane>
