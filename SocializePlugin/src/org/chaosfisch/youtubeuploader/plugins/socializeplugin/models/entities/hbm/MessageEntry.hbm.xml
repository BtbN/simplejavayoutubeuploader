<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.chaosfisch.youtubeuploader.plugins.coreplugin.models.entities.hbm">

	<class name="org.chaosfisch.youtubeuploader.plugins.socializeplugin.models.entities.MessageEntry" table="MESSAGES" schema="PUBLIC">
		<id name="identity" type="int" column="IDENTITY">
			<generator class="org.hibernate.id.IdentityGenerator"/>
		</id>
		<property name="message" type="java.lang.String" column="message"/>
		<property name="uploadID" type="int" column="uploadID"/>
		<property name="facebook" type="boolean" column="facebook"/>
		<property name="twitter" type="boolean" column="twitter"/>
		<property name="googlePlus" type="boolean" column="googlePlus"/>
		<property name="youtube" type="boolean" column="youtube"/>
	</class>

</hibernate-mapping>
